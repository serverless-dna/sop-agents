{
  "name": "@serverless-dna/sop-agents",
  "version": "0.1.1",
  "description": "Multi-agent orchestration using markdown SOPs with YAML frontmatter",
  "license": "MIT",
  "author": "Michael Walmsley",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "agent",
    "ai",
    "orchestration",
    "sop",
    "multi-agent",
    "llm",
    "bedrock",
    "strands"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/serverless-dna/sop-agents.git"
  },
  "bugs": {
    "url": "https://github.com/serverless-dna/sop-agents/issues"
  },
  "homepage": "https://github.com/serverless-dna/sop-agents#readme",
  "publishConfig": {
    "provenance": true
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check --write . && tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@strands-agents/sdk": "^0.2.0",
    "gray-matter": "^4.0.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^25.2.0",
    "@vitest/coverage-v8": "^4.0.18",
    "fast-check": "^4.5.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "sideEffects": false
}
